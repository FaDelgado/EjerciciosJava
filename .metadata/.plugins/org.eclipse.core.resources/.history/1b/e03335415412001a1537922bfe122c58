import java.util.ArrayList;

public class Ejercicios2 {

	public static void main(String[] args) {
		
		//Ejercicio 1
		System.out.println("Ejercicio 1: " + esImparMayorADiez(62));
		
		//Ejercicio 2
		System.out.println("Ejercicio 2: " + algunoMayor(6, 17, 8, 5));
		
		//Ejercicio 3
		ArrayList<Integer> array = new ArrayList<Integer>();
		array.add(10);
		array.add(20);
		array.add(30);
		array.add(40);
		array.add(50);
		
		System.out.println(array);

	}
	
	//Ejercicio 1
	static Boolean esImparMayorADiez(int num1) {
		if(num1 % 2 != 0 && num1 > 10) {
			return true;
		}else {
			return false;
		}
	}
	
	//Ejercicio 2
	static Boolean algunoMayor(int numA, int numB, int numC, int numD) {
		if((numA > numC && numA > numD) || (numB > numC && numB > numD)) {
			return true;
		}else {
			return false;
		}
	}
	
	//Ejercicio 3
	static Integer promedio(ArrayList<Integer> array) {
		int suma = 0;
		int promedio = suma / 1;
		for(int Objeto : array) {
			suma = suma + Objeto;
		}
		return suma;
	}
}
